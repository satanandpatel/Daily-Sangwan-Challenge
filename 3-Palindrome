class Solution {
    public boolean isPalindrome(int x) {

        String s = String.valueOf(x);           //converting integer(x) to string(s)

        for(int i=0;i<=(s.length())/2;i++){

            if(s.charAt(i)!=s.charAt((s.length())-1-i)){    //i==len()-1-i

                System.out.println("true");
                return false;
                
            }
            
            }
        
        return true;
    }
}
